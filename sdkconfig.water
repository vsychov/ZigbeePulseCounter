# Overrides for water meter build (apply with -DSDKCONFIG=sdkconfig.water)
# CONFIG_ZB_VARIANT_GAS is not set
CONFIG_ZB_VARIANT_WATER=y
# CONFIG_ZB_VARIANT_ELECTRIC is not set
CONFIG_ZB_DEVICE_ID=0x7771
CONFIG_PULSE_GPIO=10
CONFIG_PULSE_DEBOUNCE_MS=50
CONFIG_PULSE_PER_UNIT_NUMERATOR=10000
CONFIG_PULSE_MIN_WIDTH_MS=10
# CONFIG_BATTERY_ADC_ENABLE is not set
# CONFIG_SLEEPY_END_DEVICE is not set
CONFIG_ZB_KEEP_ALIVE_MS=30000
CONFIG_FACTORY_RESET_BUTTON_GPIO=11
# Logging: warnings and above
CONFIG_LOG_DEFAULT_LEVEL_WARN=y
# CONFIG_LOG_DEFAULT_LEVEL_INFO is not set
# CONFIG_LOG_DEFAULT_LEVEL_DEBUG is not set
# CONFIG_LOG_DEFAULT_LEVEL_VERBOSE is not set
CONFIG_LOG_DEFAULT_LEVEL=2
